/dts-v1/;
/include/ "esp32s3.dtsi"

/ {
	memory@0 {
		device_type = "memory";
		reg = <0x3d800000 0x00800000>;
	};

	soc {
		intc: intc@600c2000 {
			reg = <0x600c2800 0x800>;
		};

		ipc@600c0004 {
			wifi@1 {
				compatible = "esp,esp32-wifi-shmem";
				reg = <1>;
			};
		};

		flash: flash@42090000 {
			#address-cells = <1>;
			#size-cells = <1>;
			compatible = "mtd-rom";
			reg = <0x42090000 0x00800000>;
			bank-width = <2>;
			device-width = <2>;
			partition@0 {
				label = "linux";
				reg = <0x00000000 0x00300000>;
			};
			partition@300000 {
				label = "data";
				reg = <0x00300000 0x00400000>;
			};
		};
	};
};
